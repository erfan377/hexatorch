{
  "name": "Hexatorch",
  "description": "Chrome Extension to Protect Web3 Users from Phishing Scams!",
  "version": "1.0.3",
  "manifest_version": 3,
  "icons": {
    "8": "icon-8.png",
    "16": "icon-16.png",
    "48": "icon-32.png",
    "128": "icon-128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "React Extension",
    "default_icon": "icon-128.png"
  },
  "permissions": [
    "storage",
    "activeTab",
    "tabs",
    "alarms",
    "notifications"
  ],
  "options_page": "options.html",
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": []
}